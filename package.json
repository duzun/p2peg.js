{
  "name": "p2peg",
  "version": "0.3.5",
  "author": {
    "name": "Dumitru Uzun",
    "email": "contact@duzun.me",
    "url": "http://DUzun.Me/"
  },
  "homepage": "https://github.com/duzun/p2peg.js",
  "description": "Peer to Peer Entropy Generator (or Random numbers generator with p2p seeding)",
  "keywords": [
    "P2P",
    "random",
    "entropy",
    "cryptography",
    "javascript",
    "PHP"
  ],
  "main": "./p2peg.js",
  "files": [
    "p2peg.js",
    "lib/"
  ],
  "directories": {
    "lib": "./lib",
    "doc": "./docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duzun/p2peg.js.git"
  },
  "bugs": {
    "url": "https://github.com/duzun/p2peg.js/issues"
  },
  "license": [
    {
      "type": "MIT",
      "url": "https://github.com/duzun/p2peg.js/raw/master/LICENSE"
    }
  ],
  "readmeFilename": "README.md",
  "verup": {
    "files": [
      "p2peg.js",
      "README.md"
    ]
  },
  "scripts": {
    "test": "npm run test-node && npm run test-browser",
    "test-browser": "karma start ./spec/karma.conf.js --single-run",
    "test-node": "jasmine-node spec",
    "test-dev": "karma start ./spec/karma.conf.js",
    "verup": "verup",
    "build": "node build.js",
    "doc": "docco p2peg.js"
  },
  "jshintConfig": {
    "browser": true,
    "laxbreak": true,
    "laxcomma": true,
    "undef": true,
    "unused": true,
    "sub": true,
    "expr": true,
    "bitwise": false,
    "eqeqeq": false,
    "boss": true,
    "eqnull": true,
    "scripturl": true,
    "-W041": false,
    "-W018": false
  },
  "devDependencies": {
    "verup": "1.x",
    "docco": "0.x",
    "jasmine-core": "2.4",
    "jasmine-node": "1.14",
    "karma": "1.1",
    "karma-phantomjs-launcher": "1.x",
    "karma-chrome-launcher": "2.0.0",
    "karma-firefox-launcher": "1.x",
    "karma-safari-launcher": "1.x",
    "karma-ie-launcher": "1.x",
    "karma-jasmine": "1.x",
    "karma-requirejs": "1.x",
    "requirejs": "2.x"
  }
}
